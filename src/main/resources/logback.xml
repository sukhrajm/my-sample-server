<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d %5p %t %c:%L - %m%n</pattern>
    </encoder>
  </appender>

  <logger name="org.springframework.cloud.netflix.ribbon" level="DEBUG"/>
  <logger name="org.springframework.cloud.config" level="DEBUG"/> -->
  <!--<logger name="org.springframework.cloud.netflix.ribbon" level="DEBUG"/>
  <logger name="org.springframework.retry" level="DEBUG"/> -->
  
  <root level="INFO">
    <appender-ref ref="stdout"/>
  </root>
  
</configuration>
